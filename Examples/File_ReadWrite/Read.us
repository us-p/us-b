#project (File)Read
#main .$this
#ubit type = 0\?(function)
import (['ALL']) from M : file.('main.us')
import ? from M : error
import US_main from $project
Read == function(p1, p2) {
	if p1 & p2.([0]) ==> "" / ? error((err) : err, [{
			ubit = 1
			ubitf = 1
			kill(ubit?\ubitf : project)
		}])
	file.createM(p1 & p2.([0]), 'Read' to .$this or ...$this).[v & t : ? = {
		return v/t
		}]
}